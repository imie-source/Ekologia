<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="Ekologia"
		transaction-type="JTA">
		<jta-data-source>java:/jdbc/Ekologia</jta-data-source>
		<class>coop.ekologia.entity.user.Group</class>
		<class>coop.ekologia.entity.user.UserGroup</class>
		<class>coop.ekologia.entity.user.UserGroupPK</class>
		<exclude-unlisted-classes>false</exclude-unlisted-classes>
	</persistence-unit>
</persistence>
